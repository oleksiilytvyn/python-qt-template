    }

    QSearchEdit:disabled {
        background-color: #202020;
        color: #505050;
        }

    QSearchEdit:hover {
        border: 1px solid #242424;
        color: #b5b2b1;
        }

    QSearchEdit:selected {
        background: #202020;
        color: #202020;
        }


/** @section QTabWidget ******************************************************/

QTabWidget {
    padding: 0;
    background-color: transparent;
    selection-background-color: #505050;
    }

    QTabWidget QFrame {
        border: 0;
        padding: 0;
        margin: 0;
        background-color: transparent;
        }

    QTabWidget::pane {
        border: 1px solid #202020;
        border-radius: 0;
        padding: 0;
        margin: 0;
        background-color: #202020;
        }

    QTabWidget::pane:selected {
        background-color: #505050;
        border: 1px solid #3c3c3c;
        }


/** @section QTabBar ********************************************************/

QTabBar {
    qproperty-drawBase: 0;
    border-radius: 0;
    background-color: #202020;
    margin: 0;
    padding: 0;
    border: 0;
    }

    QTabBar::close-button {
        border: 0;
        margin: 2px;
        padding: 0;
        image: url(:/qt/close.png);
        }

    QTabBar::close-button:hover {
        image: url(:/qt/close-hover.png);
        }

    QTabBar::close-button:pressed {
        image: url(:/qt/close-pressed.png);
        }

    /* QTabBar::tab - selected ----------------------------------------------- */

    QTabBar::tab:top:selected:disabled,
    QTabBar::tab:bottom:selected:disabled,
    QTabBar::tab:left:selected:disabled,
    QTabBar::tab:right:selected:disabled {
        color: #5a5a5a;
        background-color: #202020;
        border: 0 solid transparent;
        }

    /* QTabBar::tab - !selected and disabled ---------------------------------- */

    QTabBar::tab:top:!selected:disabled,
    QTabBar::tab:bottom:!selected:disabled,
    QTabBar::tab:left:!selected:disabled,
    QTabBar::tab:right:!selected:disabled {
        color: #5a5a5a;
        background-color: #202020;
        }

    /* QTabBar::tab - selected ----------------------------------------------- */

    QTabBar::tab {
        color: #b5b2b1;
        background-color: #353535;
        border: 0 solid transparent;
        margin: 0;
        padding: 4px 10;
        min-width: 5px;
        }

    QTabBar::tab:selected {
        color: #b5b2b1;
        background-color: #505050;
        }

    QTabBar::tab:!selected:hover {
        background-color: #3c3c3c;
        }

    QTabBar::tab:left {
        margin: 0 0 1px 0;
        }

    QTabBar::tab:right {
        margin: 0 0 1px 0;
        }

    QTabBar::tab:top {
        margin: 0 1px 0 0;
        }

    QTabBar::tab:bottom {
        margin: 0 1px 0 0;
        }

    QTabBar QToolButton::right-arrow:enabled {
        image: url(:/qt/right_arrow.png);
        }

    QTabBar QToolButton::left-arrow:enabled {
        image: url(:/qt/left_arrow.png);
        }

    QTabBar QToolButton::right-arrow:disabled {
        image: url(:/qt/right_arrow_disabled.png);
        }

    QTabBar QToolButton::left-arrow:disabled {
        image: url(:/qt/left_arrow_disabled.png);
        }


/** @section QDockWidget *****************************************************/

QDockWidget {
