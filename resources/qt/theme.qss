/******************************************************************************
 *
 * Style Sheet for Qt widgets and Plugins
 * Some code from: https://github.com/ColinDuquesnoy/QDarkStyleSheet
 *
 *****************************************************************************/


/** @section QWidget *********************************************************/
QWidget {
    font-size: 14pt;
    }

QColorDialog,
QFileDialog,
QFontDialog,
QMainWindow,
QDialog {
    background-color: #505050;
    border: 0 solid transparent;
    padding: 0;
    color: #b5b2b1;
    selection-background-color: #5a5a5a;
    selection-color: #b5b2b1;
    }


/** @section QMainWindow *****************************************************/
/* This adjusts the splitter in the dock widget, not QSplitter               */

QMainWindow::separator {
    background-color: #202020;
    border: 0 solid #202020;
    spacing: 0;
    padding: 0;
    }

    QMainWindow::separator:hover {
        background-color: #353535;
        border: 0 solid #202020;
        }

    QMainWindow::separator:horizontal {
        width: 2px;
        margin-top: 2px;
        margin-bottom: 2px;
        image: url(:/qt/toolbar-v-separator.png);
        }

    QMainWindow::separator:vertical {
        height: 2px;
        margin-left: 2px;
        margin-right: 2px;
        image: url(:/qt/toolbar-h-separator.png);
        }


/** @section QSplitter *******************************************************/

QSplitter {
    background-color: #202020;
    spacing: 0;
    padding: 0;
    margin: 0;
    }

    QSplitter::handle {
        background-color: #202020;
        }

    QSplitter::handle:active,
    QSplitter::handle:pressed {
        background-color: #242424;
        }

    QSplitter::handle:horizontal {
        width: 2px;
        }

    QSplitter::handle:vertical {
        height: 2px;
        }


/** @section QToolTip ********************************************************/

QToolTip {
    background-color: #202020;
    border: 1px solid #353535;
    border-radius: 0;
    color: #b5b2b1;
    padding: 0;   /*remove padding, for fix combo box tooltip*/
    opacity: 230; /*reducing transparency to read better*/
    }


/** @section QStatusBar ******************************************************/

QStatusBar {
    background-color: #202020;
    color: #b5b2b1;
    border: 1px solid #202020;
    }


/** @section QCheckBox *******************************************************/

QCheckBox {
    background-color: #505050;
    color: #f1f1f1;
    spacing: 4px;
    outline: none;
    padding-top: 4px;
    padding-bottom: 4px;
    }

    QCheckBox:focus {
        border: none;
        }

    QCheckBox:disabled,
    QCheckBox QWidget:disabled {
        background-color: #505050;
        color: #353535;
        }

    QCheckBox::indicator {
        margin-left: 4px;
        width: 16px;
        height: 16px;
        }

    QCheckBox::indicator:unchecked {
        image: url(:/qt/checkbox_unchecked.png);
        }

    QCheckBox::indicator:unchecked:hover,
    QCheckBox::indicator:unchecked:focus,
    QCheckBox::indicator:unchecked:pressed {
        border: none;
        image: url(:/qt/checkbox_unchecked_focus.png);
        }

    QCheckBox::indicator:unchecked:disabled {
        image: url(:/qt/checkbox_unchecked_disabled.png);
        }

    QCheckBox::indicator:checked {
        image: url(:/qt/checkbox_checked.png);
        }

    QCheckBox::indicator:checked:hover,
    QCheckBox::indicator:checked:focus,
    QCheckBox::indicator:checked:pressed {
        border: none;
        image: url(:/qt/checkbox_checked_focus.png);
        }

    QCheckBox::indicator:checked:disabled{
        image: url(:/qt/checkbox_checked_disabled.png);
        }

    QCheckBox::indicator:indeterminate {
        image: url(:/qt/checkbox_indeterminate.png);
        }

    QCheckBox::indicator:indeterminate:disabled {
        image: url(:/qt/checkbox_indeterminate_disabled.png);
        }

    QCheckBox::indicator:indeterminate:focus,
    QCheckBox::indicator:indeterminate:hover,
    QCheckBox::indicator:indeterminate:pressed {
        image: url(:/qt/checkbox_indeterminate_focus.png);
        }


/** @section QGroupBox *******************************************************/

QGroupBox {
    font-weight: bold;
    border: 1px solid #505050;
    border-radius: 0;
    padding: 4px;
    margin-top: 16px;
    }

    QGroupBox::title {
        subcontrol-origin: margin;
        subcontrol-position: top left;
        left: 3px;
        padding: 8px 5px 16px 3px;
    }

    QGroupBox::indicator {
        margin-left: 4px;
        width: 16px;
        height: 16px;
        }

    QGroupBox::indicator {
        margin-left: 2px;
        }

    QGroupBox::indicator:unchecked:hover,
    QGroupBox::indicator:unchecked:focus,
    QGroupBox::indicator:unchecked:pressed {
        border: none;
        image: url(:/qt/checkbox_unchecked_focus.png);
        }

    QGroupBox::indicator:checked:hover,
    QGroupBox::indicator:checked:focus,
    QGroupBox::indicator:checked:pressed {
        border: none;
        image: url(:/qt/checkbox_checked_focus.png);
        }

    QGroupBox::indicator:checked:disabled {
        image: url(:/qt/checkbox_checked_disabled.png);
        }

    QGroupBox::indicator:unchecked:disabled {
        image: url(:/qt/checkbox_unchecked_disabled.png);
        }


/** @section QRadioButton ****************************************************/

QRadioButton {
    background-color: #505050;
    color: #b5b2b1;
    spacing: 0;
    padding: 0;
    border: none;
    outline: none;
    }

    QRadioButton:focus {
        border: none;
        }

    QRadioButton:disabled {
        background-color: #505050;
        color: #353535;
        border: none;
        outline: none;
        }

    QRadioButton QWidget {
        background-color: #202020;
        color: #b5b2b1;
        spacing: 0;
        padding: 0;
        outline: none;
        border: none;
        }

    QRadioButton::indicator {
        border: none;
        outline: none;
        margin-bottom: 2px;
        width: 25px;
        height: 25px;
        }

    QRadioButton::indicator:unchecked {
        image: url(:/qt/radio_unchecked.png);
        }

    QRadioButton::indicator:unchecked:hover,
    QRadioButton::indicator:unchecked:focus,
    QRadioButton::indicator:unchecked:pressed {
        border: none;
        outline: none;
        image: url(:/qt/radio_unchecked_focus.png);
        }

    QRadioButton::indicator:checked {
        border: none;
        outline: none;
        image: url(:/qt/radio_checked.png);
        }

    QRadioButton::indicator:checked:hover,
    QRadioButton::indicator:checked:focus,
    QRadioButton::indicator:checked:pressed {
        border: none;
        outline: none;
        image: url(:/qt/radio_checked_focus.png);
        }

    QRadioButton::indicator:checked:disabled {
        outline: none;
        image: url(:/qt/radio_checked_disabled.png);
        }

    QRadioButton::indicator:unchecked:disabled {
        image: url(:/qt/radio_unchecked_disabled.png);
        }


/** @section QMenuBar ********************************************************/

QMenuBar {
    background-color: #202020;
    padding: 0;
    border: 0 solid #202020;
    color: #b5b2b1;
    }

    QMenuBar:focus {
        border: 0 solid #505050;
        }

    QMenuBar::item {
        background: transparent;
        padding: 4px;
        }

    QMenuBar::item:selected {
        padding: 4px;
        background: #353535;
        border: 0 solid #202020;
        }

    QMenuBar::item:pressed {
        padding: 4px;
        border: 0 solid #202020;
        background-color: #353535;
        color: #b5b2b1;
        }


/** @section QWidget *********************************************************/


QMenu {
    border: 1px solid #505050;
    background-color: #202020;
    color: #b5b2b1;
    margin: 0;
    }

    QMenu::separator {
        height: 1px;
        background-color: #353535;
        color: #b5b2b1;
        padding: 0;
        margin: 0;
        }

    QMenu::icon {
        margin: 0;
        padding-left:4px;
        }

    QMenu::item {
        padding: 4px;
        border: none;
        background-color: #202020;
        }

    QMenu::item:selected {
        color: #f1f1f1;
        background-color: #353535;
        }

    QMenu::item:disabled {
        color: #3c3c3c;
        }

    QMenu::indicator {
        width: 12px;
        height: 12px;
        padding: 6px;
        }

    QMenu::item:checked,
    QMenu::item:unchecked {
        padding-left: 4px;
        }

    QMenu::indicator:non-exclusive:unchecked {
        image: url(:/qt/checkbox_unchecked.png);
        }

    QMenu::indicator:non-exclusive:unchecked:selected {
        image: url(:/qt/checkbox_unchecked_disabled.png);
        }

    QMenu::indicator:non-exclusive:checked {
        image: url(:/qt/checkbox_checked.png);
        }

    QMenu::indicator:non-exclusive:checked:selected {
        image: url(:/qt/checkbox_checked_disabled.png);
        }

    QMenu::indicator:exclusive:unchecked {
        image: url(:/qt/radio_unchecked.png);
        }

    QMenu::indicator:exclusive:unchecked:selected {
        image: url(:/qt/radio_unchecked_disabled.png);
        }

    QMenu::indicator:exclusive:checked {
        image: url(:/qt/radio_checked.png);
        }

    QMenu::indicator:exclusive:checked:selected {
        image: url(:/qt/radio_checked_disabled.png);
        }

    QMenu::right-arrow {
        margin: 5px;
        image: url(:/qt/right_arrow.png)
        }



/** @section QAbstractItemView ***********************************************/

QAbstractItemView {
    background: #505050;
    alternate-background-color: #5a5a5a;
    color: #b5b2b1;
    border: 1px solid #505050;
    }

    QAbstractItemView::item {
        min-height: 24px;
        }

    QAbstractItemView QLineEdit {
        padding: 2px;
        }


/** @section QAbstractScrollArea *********************************************/

QAbstractScrollArea {
    background-color: #505050;
    border: 0 solid #505050;
    padding: 4px;
    color: #b5b2b1;
    }

    QAbstractScrollArea:disabled {
        color: #f1f1f1;
        }


/** @section QScrollArea *****************************************************/

QScrollArea {
    background: #202020;
    padding: 0;
    }

    QScrollArea QWidget {
        background-color: #202020;
        }

    QScrollArea QWidget QWidget {
        background-color: #202020;
        }

    QScrollArea:disabled {
        background: #5a5a5a;
        }

    QScrollArea QWidget QWidget:disabled {
        background-color: #5a5a5a;
        }


/** @section QScrollBar *********************************************************/

QScrollBar:horizontal {
    background: transparent;
    border: none;
    height: 8px;
    margin: 4px 4px 0 4px;
    }

    QScrollBar::handle:horizontal {
        min-width: 20;
        border-radius: 3px;
        }

    QScrollBar::add-line:horizontal {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
        }

    QScrollBar::sub-line:horizontal {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: top;
        subcontrol-origin: margin;
        }

    QScrollBar::up-arrow:horizontal,
    QScrollBar::down-arrow:horizontal {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

    QScrollBar::add-page:horizontal,
    QScrollBar::sub-page:horizontal {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

    QScrollBar::handle:vertical,
    QScrollBar::handle:horizontal {
        background: rgba(255, 255, 255, 0.7);
    }

QScrollBar:vertical {
    background: transparent;
    border: none;
    width: 8px;
    margin: 4px 4px 4px 0;
    }

    QScrollBar::handle:vertical {
        min-height: 20;
        border-radius: 3px;
        }

    QScrollBar::add-line:vertical {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
        }

    QScrollBar::sub-line:vertical {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: top;
        subcontrol-origin: margin;
        }

    QScrollBar::up-arrow:vertical,
    QScrollBar::down-arrow:vertical {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

    QScrollBar::add-page:vertical,
    QScrollBar::sub-page:vertical {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

/** @section QTextEdit *******************************************************/

QTextEdit {
    background-color: #202020;
    color: #b5b2b1;
    border: 1px solid #505050;
    }

    QTextEdit:hover {
        border: 1px solid #242424;
        color: #b5b2b1;
        }

    QTextEdit:selected {
        background: #3c3c3c;
        color: #505050;
        }

    QTextEdit:disabled {
        background: #202020;
        color: #505050;
        }


/** @section QPlainTextEdit **************************************************/

QPlainTextEdit {
    background-color: #202020;
    color: #b5b2b1;
    border-radius: 0;
    border: 1px solid #505050;
    }

    QPlainTextEdit:hover {
        border: 1px solid #242424;
        color: #b5b2b1;
        }

    QPlainTextEdit:selected {
        background: #3c3c3c;
        color: #505050;
        }

    QPlainTextEdit:disabled {
        background: #202020;
        color: #505050;
        }

    QPlainTextEdit:disabled:hover {
        border: 1px solid transparent;
        }

/** @section QSizeGrip *******************************************************/

QSizeGrip {
    image: url(:/qt/sizegrip.png);
    width: 12px;
    height: 12px;
    background-color: transparent;
    }


/** @section QStackedWidget **************************************************/

QStackedWidget {
    padding: 0;
    border: none;
    }


/** @section QToolBar ********************************************************/

QToolBar {
    background-color: #202020;
    padding: 3px;
    min-height: 34px;
    border-top: 1px solid #353535;
    }

    QToolBar QToolButton {
        background-color: #202020;
        color: #b5b2b1;
        }

    QToolBar QLabel {
        background-color: #202020;
        color: #b5b2b1;
        border: 0 solid transparent;
        outline: none;
        }

    QToolBar::handle:horizontal {
        width: 6px;
        image: url(:/qt/toolbar-h-move.png);
        }

    QToolBar::handle:vertical {
        height: 6px;
        image: url(:/qt/toolbar-v-move.png);
        }

    QToolBar::separator:horizontal {
        width: 3px;
        image: url(:/qt/toolbar-h-separator.png);
        }

    QToolBar::separator:vertical {
        height: 3px;
        image: url(:/qt/toolbar-v-separator.png);
        }

    QToolButton#qt_toolbar_ext_button {
        background: #202020;
        border: 0;
        color: #b5b2b1;
        image: url(:/qt/right_arrow.png);
        }


/** @section QAbstractSpinBox ************************************************/

QAbstractSpinBox {
    background-color: #202020;
    border: 1px solid #505050;
    color: #b5b2b1;
    padding: 2px 4px;
    border-radius: 0;
    }

    QAbstractSpinBox:up-button {
        background-color: transparent #202020;
        subcontrol-origin: border;
        subcontrol-position: top right;
        border-left: 1px solid #505050;
        margin: 1px;
        }

    QAbstractSpinBox::up-arrow,
    QAbstractSpinBox::up-arrow:disabled,
    QAbstractSpinBox::up-arrow:off {
        image: url(:/qt/up_arrow_disabled.png);
        width: 9px;
        height: 9px;
        }

    QAbstractSpinBox::up-arrow:hover {
        image: url(:/qt/up_arrow.png);
        }

    QAbstractSpinBox:down-button {
        background-color: transparent #202020;
        subcontrol-origin: border;
        subcontrol-position: bottom right;
        border-left: 1px solid #505050;
        margin: 1px;
        }

    QAbstractSpinBox::down-arrow,
    QAbstractSpinBox::down-arrow:disabled,
    QAbstractSpinBox::down-arrow:off {
        image: url(:/qt/down_arrow_disabled.png);
        width: 9px;
        height: 9px;
        }

    QAbstractSpinBox::down-arrow:hover {
        image: url(:/qt/down_arrow.png);
        }

    QAbstractSpinBox:hover {
        border: 1px solid #242424;
        color: #b5b2b1;
        }

    QAbstractSpinBox:selected {
        background: #3c3c3c;
        color: #505050;
        }

    QAbstractSpinBox:disabled {
        background: #202020;
        color: #505050;
        }


/** @section QLabel **********************************************************/

QLabel {
    border: none;
    padding: 0;
    margin: 0;
    color: #b5b2b1;
    background-color: transparent;
    }

    QLabel::disabled {
        color: #353535;
        }


/** @section QTextBrowser ****************************************************/

QTextBrowser {
    background-color: transparent;
    border: 0 solid #505050;
    color: #b5b2b1;
    border-radius: 0;
    }

    QTextBrowser:disabled {
        background-color: #202020;
        border: 1px solid #505050;
        color: #f1f1f1;
        border-radius: 0;
        }

    QTextBrowser:hover,
    QTextBrowser:!hover,
    QTextBrowser::selected,
    QTextBrowser::pressed {
        border: 1px solid #505050;
        }


/** @section QGraphicsView ***************************************************/

QGraphicsView {
    background: #202020;
    border: none;
    color: #b5b2b1;
    border-radius: 0;
    padding: 0;
    margin: 0;
    }

    QGraphicsView:disabled {
        background: #202020;
        }


/** @section QCalendarWidget *************************************************/

QCalendarWidget {
    border: 1px solid #505050;
    border-radius: 0;
    }

    QCalendarWidget:disabled {
        background-color: #202020;
        color: #353535;
        }


/** @section QLCDNumber ******************************************************/

QLCDNumber {
    background-color: #505050;
    color: #b5b2b1;
    }

    QLCDNumber:disabled {
        background-color: #505050;
        color: #353535;
        }


/** @section QProgressBar ****************************************************/

QProgressBar {
    background-color: #353535;
    border: 1px solid #353535;
    color: #f1f1f1;
    border-radius: 0;
    text-align: center;
    }

    QProgressBar:disabled {
        background-color: #353535;
        border: 1px solid #353535;
        color: #f1f1f1;
        border-radius: 0;
        text-align: center;
        }

    QProgressBar::chunk {
        background-color: #242424;
        color: #202020;
        border-radius: 0;
        }

    QProgressBar::chunk:disabled {
        background-color: #3c3c3c;
        color: #f1f1f1;
        border-radius: 0;
        }


/** @section QPushButton *****************************************************/

QPushButton {
    background-color: #353535;
    border: 1px solid #353535;
    color: #f1f1f1;
    border-radius: 0;
    padding: 3px 5px 4px 5px;
    outline: none;
    font-weight: normal;
    }

    QPushButton:disabled {
        background-color: #505050;
        border: 1px solid #353535;
        color: #353535;
        border-radius: 0;
        padding: 3px 5px 4px 5px;
        }

    QPushButton:checked {
        background-color: #242424;
        border: 1px solid #242424;
        color: #202020;
        border-radius: 0;
        outline: none;
        }

    QPushButton:checked:disabled {
        background-color: #353535;
        border: 1px solid #353535;
        color: #505050;
        border-radius: 0;
        padding: 3px 5px 4px 5px;
        outline: none;
        }

    QPushButton::menu-indicator {
        subcontrol-origin: padding;
        subcontrol-position: bottom right;
        bottom: 5px;
        }

    QPushButton:pressed {
        background-color: #242424;
        border: 1px solid #242424;
        color: #202020;
        }

    QPushButton:hover,
    QPushButton:checked:hover {
        background-color: #242424;
        border: 1px solid #242424;
        color: #202020;
        }

    QPushButton:selected,
    QPushButton:checked:selected {
        background-color: #242424;
        border: 1px solid #242424;
        color: #202020;
        }

QDialogButtonBox QPushButton,
QDialogButtonBox QPushButton:disabled {
    padding: 4px 7px;
    }

/** @section QToolButton *****************************************************/

QToolButton {
    background-color: transparent;
    margin: 0;
    padding: 4px;
    border: 1px solid transparent;
    }

    QToolButton:disabled {
        background-color: #505050;
        color: #353535;
        border: 1px solid #353535;
        }

    /* the sub-controls below are used only in the MenuButtonPopup mode */

    QToolButton[popupMode="1"] {
        padding: 2px 12px 2px 2px; /* only for MenuButtonPopup */
        border: 1px solid #505050;   /* make way for the popup button */
        border-radius: 0;
    }

    /* The sub-control below is used only in the InstantPopup or DelayedPopup mode */
    QToolButton[popupMode="2"] {
        padding: 2px 12px 2px 2px; /* only for InstantPopup */
        border: 1px solid #505050;    /* make way for the popup button */
        }

    QToolButton::menu-button {
        padding: 2px;
        border-radius: 0;
        border: 1px solid #505050;
        /* 16px width + 4px for border = 20 allocated above */
        width: 16px;
        outline: none;
        }

    QToolButton::menu-button:hover,
    QToolButton::menu-button:checked:hover {
        border: 1px solid #242424;
        }

    QToolButton::menu-indicator {
        image: url(:/qt/down_arrow.png);
        top: -8px;     /* shift it a bit */
        left: -4px;    /* shift it a bit */
        }

    QToolButton::menu-arrow {
        image: url(:/qt/down_arrow.png);
        }

    QToolButton::menu-arrow:open {
        border: 1px solid #505050;
        }


/** @section QCommandLinkButton **********************************************/

QCommandLinkButton {
    background-color: #353535;
    border: 1px solid #353535;
    color: #f1f1f1;
    border-radius: 0;
    padding: 0;
    margin: 0;
    }

    QCommandLinkButton:hover,
    QCommandLinkButton:active {
        background-color: #242424;
        border: 1px solid #242424;
        color: #202020;
        }

    QCommandLinkButton:disabled {
        background-color: transparent;
        border: 1px solid #353535;
        color: #353535;
        }


/** @section QComboBox *******************************************************/

QComboBox {
    border: 1px solid #353535;
    background-color: #353535;
    border-radius: 0;
    selection-background-color: #353535;
    padding: 4px;
    outline: none;
    color: #f1f1f1;
    }

    QComboBox:disabled {
        background-color: #505050;
        color: #353535;
        }

    QComboBox:hover,
    QComboBox:on {
        border: 1px solid #242424;
        background: #242424;
        selection-background-color: #242424;
        selection-color: #202020;
        color: #202020;
        }

    QComboBox:editable,
    QComboBox:editable:hover {
        background: #353535;
        color: #f1f1f1;
        }

    QComboBox::drop-down {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 20;
        border-left-width: 0;
        border-left-color: #505050;
        border-left-style: solid;
        }

    QComboBox::down-arrow {
