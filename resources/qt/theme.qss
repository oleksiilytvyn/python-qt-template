/** @section QSlider *********************************************************/

QSlider {
    background: transparent;
    }

    QSlider:disabled {
        background: transparent;
        }

    QSlider:focus {
        border: none;
        }

QSlider::groove:horizontal {
    background: #3c3c3c;
    border: 0 solid #3c3c3c;
    height: 5px;
    margin: 0;
    border-radius: 0;
    }

QSlider::sub-page:horizontal {
    background: #5a5a5a;
    border: 0 solid #5a5a5a;
    height: 4px;
    margin: 0;
    border-radius: 0;
    }

QSlider::sub-page:horizontal:disabled {
    background: #3c3c3c;
    }

QSlider::handle:horizontal {
    background: #242424;
    border: 3px solid #242424;
    width: 8px;
    height: 0;
    margin: 0 0;
    border-radius: 0;
    }

QSlider::handle:horizontal:hover {
    background: #242424;
    }

/* Vertical */

QSlider::groove:vertical {
    background: #3c3c3c;
    border: 0 solid #3c3c3c;
    width: 4px;
    margin: 0;
    border-radius: 0;
    }

QSlider::sub-page:vertical {
    background: #5a5a5a;
    border: 0 solid #5a5a5a;
    width: 4px;
    margin: 0;
    border-radius: 0;
    }

QSlider::sub-page:vertical:disabled {
    background: #5a5a5a;
    }

QSlider::handle:vertical {
    background: #242424;
    border: 1px solid #242424;
    width: 0;
    height: 8px;
    margin: 0 0;
    border-radius: 0;
    }

QSlider::handle:vertical:hover {
    background: #242424;
    border: 1px solid #242424;
    }


/** @section QLineEdit *******************************************************/

QLineEdit {
